import { configureStore } from '@reduxjs/toolkit'
import counterReducer from './counterSlice{% if values.useTypeScript %}.ts{% else %}.js{% endif %}'

export const store = configureStore({
  reducer: {
    counter: counterReducer,
  },
})

{% if values.useTypeScript %}export type RootState = ReturnType<typeof store.getState>
export type AppDispatch = typeof store.dispatch{% endif %}